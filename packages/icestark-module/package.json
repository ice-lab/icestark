{
  "name": "@ice/stark-module",
  "version": "1.0.0",
  "description": "toolkit for load standard micro-module",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "watch": "tsc -w",
    "prepublishOnly": "npm run test && npm run build",
    "test": "NODE_ENV=unittest jest",
    "coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ice-lab/icestark.git"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "registry": "http://registry.npmjs.com/"
  },
  "keywords": [
    "modules",
    "icestark"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ice-lab/icestark/issues"
  },
  "homepage": "https://github.com/ice-lab/icestark#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0"
  },
  "devDependencies": {
    "codecov": "^3.4.0",
    "jest": "^25.2.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "preset": "ts-jest"
  }
}
